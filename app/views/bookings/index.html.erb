<h1> All Bookings <h1>

  <table>
    <thead>
      <tr class="booking_heading">
        <th>User id </th>
        <th>Sufboards id </th>
        <th>Start date </th>
        <th>End date </th>
        <th>Total price </th>
      <tr>
    <thead>

    <tbody class="booking_outcome">
        <% @bookings.each do |booking| %>
          <tr class="ashley">
            <td><%= booking.id %></td>
            <td><%= booking.surfboard_id%></td>
            <td><%= booking.start_date%></td>
            <td><%= booking.end_date%></td>
            <td><%= booking.total_price%></td>
          </tr>
        <% end %>
    <tbody>

  </table>
